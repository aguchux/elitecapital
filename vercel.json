  {
    "version": 2,
    "name": "elitecapital",
    "builds": [
       { "src": "server.ts", "use": "@vercel/node" }
    ],
    "routes": [
       { "src": "/(.*)", "dest": "/server.ts" }
    ],
    "crons": [
      {
        "path": "/crons",
        "schedule": "0 2 */13 * *"
      }
    ],
    "installCommand": "npm install",
    "outputDirectory": "build",
    "buildCommand": "npm run build && prisma generate && prisma migrate deploy"
 }